{
  "expo": {
    "extra": {
      "authSession": {
        "redirectUri": "https://auth.expo.io/@archbishop/c6-companion"
      },
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3271ed2-47da-4e33-9d3f-649ed63f618f"
      }
    },
    "name": "c6-companion",
    "slug": "c6-companion",
    "owner": "archbishop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "c6-companion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.archbishop.c6companion"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.archbishop.c6companion"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.c6companion"
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow this app to access your camera",
          "microphonePermission": "Allow this app to access your microphone",
          "photoLibraryPermission": "Allow this app to access your photo library"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a3271ed2-47da-4e33-9d3f-649ed63f618f"
    }
  }
}
